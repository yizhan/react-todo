{
    "routes": [
      {
        "route": "/*",
        "rewrite": "/index.html",
        "statusCode": 200
      }
    ],
    "navigationFallback": {
        "rewrite": "index.html",
        "exclude": ["/images/*.{png,jpg,gif}", "/css/*"]
      },
    
    "mimeTypes": {
      ".json": "application/json"
    }
  }